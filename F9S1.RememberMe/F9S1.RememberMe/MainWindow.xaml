<Window x:Class="F9S1.RememberMe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="313" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="0" />
                <GradientStop Color="#FF2A36AB" Offset="1" />
            </LinearGradientBrush>
        </Grid.Background>
        <ListBox Margin="12,12,12,126" Name="outputBox" Foreground="#FFE10000" FontFamily="Courier New"></ListBox>
        <Label Margin="12,213,12,12" Name="displayBox" Foreground="#FFEB1919" FontWeight="Bold" />
        <TextBox Margin="12,157,12,65" Name="inputBox" KeyDown="inputBox_KeyDown" Foreground="#FF19580E" />
    </Grid>
</Window>
