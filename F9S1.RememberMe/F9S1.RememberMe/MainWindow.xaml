<Window x:Class="F9S1.RememberMe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="331" Width="525" Loaded="Window_Loaded" Closing="OnClose" StateChanged="OnStateChanged" IsVisibleChanged="OnIsVisibleChanged">
    <Window.CommandBindings>
        <CommandBinding Command="Undo"
                        CanExecute="CommandBinding_CanExecute"
                        Executed="undoExecuted"/>
        <CommandBinding Command="Redo"
                        CanExecute="CommandBinding_CanExecute"
                        Executed="redoExecuted"/>
    </Window.CommandBindings>
    <Window.InputBindings>
        <KeyBinding Command="Undo" Key="Z" Modifiers="Ctrl"/>
        <KeyBinding Command="Redo" Key="Y" Modifiers="Ctrl"/>
    </Window.InputBindings>
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/F9S1.RememberMe;component/Images/large_Abstract_Dark_Gray_Paper_Rape_Wallpaper_86207.jpg" />
        </Grid.Background>
        <ListBox Margin="12,0,12,126" Name="outputBox" Foreground="#FFE10000" FontFamily="Courier New" Height="136" VerticalAlignment="Bottom"></ListBox>
        <Label Margin="12,0,12,12" Name="displayBox" Foreground="#FFEB1919" FontWeight="Bold" Height="49" VerticalAlignment="Bottom" />
        <TextBox Margin="12,0,12,65" Name="inputBox" KeyDown="inputBox_KeyDown" Foreground="#FF19580E" TextChanged="inputBox_TextChanged" PreviewKeyDown="inputBox_PreviewKeyDown" Height="52" VerticalAlignment="Bottom" />
        <Label Content="*" Height="28" HorizontalAlignment="Left" Margin="12,0,0,262" Name="labelStar" VerticalAlignment="Bottom" FontWeight="Bold" Foreground="#FFF8F2F2"></Label>
        <Label Content="DETAILS" Height="28" HorizontalAlignment="Left" Margin="112,0,0,262" Name="labelDetails" VerticalAlignment="Bottom" Width="61" FontWeight="Bold" Foreground="#FFF8F2F2"></Label>
        <Label Content="DEADLINE" Height="28" HorizontalAlignment="Left" Margin="263,0,0,262" Name="label2" VerticalAlignment="Bottom" Width="78" FontWeight="Bold" Foreground="#FFF8F2F2"></Label>
        <Label Content="LABEL" Height="28" HorizontalAlignment="Right" Margin="0,0,78,262" Name="label3" VerticalAlignment="Bottom" Width="51" FontWeight="Bold" Foreground="#FFF8F2F2"></Label>
        <Label Content="ARCH." Height="28" HorizontalAlignment="Right" Margin="0,0,12,262" Name="label4" VerticalAlignment="Bottom" Width="47" FontWeight="Bold" Foreground="#FFF8F2F2"></Label>
    </Grid>
</Window>
